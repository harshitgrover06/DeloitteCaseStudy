@{
    ViewData["Title"] = "UploadFile";
    Layout = "/Views/Shared/_Layout.cshtml";
    string[] TableHeaders = new string[]{ 
      " Name "
      ," UserId "
      ," Created On "
      ," Download Link"
    };
}

<h1>UploadFile</h1>

<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="UploadFile" onload="getFileDate()">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload power Bi files using this form:</p>
            <button class="btn btn-primary"><input type="file" name="files" id="myFile" accept=".pbix" onchange="getFileDate()" required /></button>
        </div>
    </div>
    <br /><br />
    <div class="form-group">
        <div class="col-md-10">
            <button class="btn btn-primary"><input type="submit" value="Upload" /></button>
        </div>
    </div>
    <input name="metadata" type="datetime-local" id="metadata" hidden/>
</form>


